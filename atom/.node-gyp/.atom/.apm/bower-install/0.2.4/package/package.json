{
  "name": "bower-install",
  "main": "./index",
  "version": "0.2.4",
  "private": true,
  "description": "Automatically install and save any missing bower components being used in the current bower.json file",
  "repository": "https://github.com/gdi2290/atom-bower-install",
  "license": "MIT",
  "activationEvents": [
    "bower-install:save",
    "bower-install:save-dev"
  ],
  "engines": {
    "atom": ">0.50.0"
  },
  "dependencies": {
    "remove-element": "0.0.0",
    "safe-json-parse": "~1.0.1",
    "resolve": "~0.6.1",
    "coffee-script": "~1.7.1",
    "ansi-html-stream": "0.0.3",
    "detective": "~2.4.0",
    "findup": "~0.1.3",
    "underscore": "~1.6.0",
    "htmlparser2": "~3.5.1"
  }
}
